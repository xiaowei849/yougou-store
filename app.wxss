@import "./styles/iconfont.wxss";

/* 微信小程序中不支持通配符星号 */
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 
  主题颜色 通过变量来实现
*/
page {
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  /* 统一字体大小 */
  font-size: 28rpx;
}

image{
  width: 100%;
}